@model ViEdu.Data.Entities.Position
@{
    ViewData["Title"] = "Position";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="container">
    <h1 class="mt-4">Chi tiết</h1>
    <div class="card mb-4">
        <div class="card-header">
            <a class="btn btn-success" asp-action="Index" asp-controller="Position">Về danh sách</a>
        </div>
        <div class="card-body">
            <div class="col-md-12">
                <dl class="row">
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.ID)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.ID)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.POSITION_CD)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.POSITION_CD)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.NAME)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.NAME)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.DESC)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.DESC)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.RANK)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.RANK)
                    </dd>
                    <dt class="col-sm-2">
                        @Html.DisplayNameFor(model => model.CREATED_DATE)
                    </dt>
                    <dd class="col-sm-10">
                        @Html.DisplayFor(model => model.CREATED_DATE)
                    </dd>

                </dl>
            </div>
        </div>
    </div>
</div>
